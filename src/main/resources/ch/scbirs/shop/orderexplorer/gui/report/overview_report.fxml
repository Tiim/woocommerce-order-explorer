<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" minHeight="-Infinity" minWidth="-Infinity" prefHeight="594.0"
            prefWidth="841.0" xmlns="http://javafx.com/javafx/8.0.121">
    <top>
        <Label fx:id="title" styleClass="title" text="Title" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </Label>
    </top>
    <center>
        <TableView fx:id="table" focusTraversable="false" prefHeight="200.0" prefWidth="200.0" styleClass="table"
                   BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="firstName" prefWidth="75.0" text="First Name"/>
                <TableColumn fx:id="lastName" prefWidth="75.0" text="Last Name"/>
                <TableColumn fx:id="itemName" prefWidth="75.0" text="Item"/>
                <TableColumn fx:id="quantity" prefWidth="75.0" text="Quantity"/>
                <TableColumn fx:id="sku" prefWidth="75.0" text="SKU"/>
                <TableColumn fx:id="meta" prefWidth="75.0" text="Meta"/>
                <TableColumn fx:id="price" prefWidth="75.0" text="Price"/>
            </columns>
        </TableView>
    </center>
</BorderPane>
