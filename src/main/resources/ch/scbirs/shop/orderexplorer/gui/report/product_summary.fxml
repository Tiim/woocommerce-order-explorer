<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="594.0" prefWidth="420.0"
            xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="ch.scbirs.shop.orderexplorer.gui.report.ProductSummary">
    <top>
        <Label fx:id="title" styleClass="title" text="Title" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </BorderPane.margin>
        </Label>
    </top>
    <center>
        <TableView prefHeight="200.0" prefWidth="200.0" styleClass="table" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="number" editable="false" prefWidth="75.0" sortable="false" text="Number"/>
                <TableColumn fx:id="name" editable="false" prefWidth="75.0" sortable="false" text="Name"/>
                <TableColumn fx:id="sku" editable="false" prefWidth="75.0" sortable="false" text="SKU"/>
                <TableColumn fx:id="meta" editable="false" prefWidth="75.0" sortable="false" text="Meta"/>
                <TableColumn fx:id="price" editable="false" prefWidth="75.0" sortable="false" text="Price"/>
                <TableColumn fx:id="total" editable="false" prefWidth="75.0" sortable="false" text="Total Price"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </TableView>
    </center>
</BorderPane>
