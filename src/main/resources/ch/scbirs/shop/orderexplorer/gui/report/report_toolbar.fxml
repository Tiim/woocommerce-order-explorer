<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" type="BorderPane"
         xmlns="http://javafx.com/javafx/8.0.121">
    <top>
        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0"
              BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#print" text="Print"/>
                <Button mnemonicParsing="false" onAction="#printPdf" text="Print to Pdf"/>
                <Button mnemonicParsing="false" onAction="#exportImpl" text="Export"/>
            </children>
        </HBox>
    </top>
</fx:root>
