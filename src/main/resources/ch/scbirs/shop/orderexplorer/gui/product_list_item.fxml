<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <ImageView fx:id="img" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
   </left>
   <center>
       <VBox BorderPane.alignment="CENTER">
         <children>
             <HBox alignment="CENTER_LEFT">
                 <children>
                     <Label fx:id="quantity" styleClass="bold" text="Quantity">
                         <opaqueInsets>
                             <Insets/>
                         </opaqueInsets>
                         <HBox.margin>
                             <Insets right="10.0"/>
                         </HBox.margin>
                     </Label>
                     <Label fx:id="name" styleClass="bold" text="Name"/>
                 </children>
             </HBox>
             <Label fx:id="price" styleClass="price" text="Price"/>
             <Label fx:id="sku" text="SKU"/>
             <Label fx:id="meta" text="Meta">
                 <VBox.margin>
                     <Insets top="25.0"/>
                 </VBox.margin>
             </Label>
         </children>
       </VBox>
   </center>
</BorderPane>
